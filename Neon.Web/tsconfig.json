{
  "compilerOptions": {
    //"noEmit": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Node",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "paths": {
      "*": [
        "wwwroot/ts/modules/*",
        "wwwroot/svg/*"
      ]
    },
    "strict": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": true,
    //"sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true
  }
}